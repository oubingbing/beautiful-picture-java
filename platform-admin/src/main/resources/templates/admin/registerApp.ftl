<!DOCTYPE html><html><head>    <meta charset="UTF-8">    <title>注册小程序</title>    <meta name="renderer" content="webkit">    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">    <meta name="viewport" content="width=device-width,user-scalable=yes, minimum-scale=0.4, initial-scale=0.8,target-densitydpi=low-dpi" />    <link rel="shortcut icon" href="/favicon.ico" type="image/x-icon" />    <link rel="stylesheet" href="/static/css/font.css">    <link rel="stylesheet" href="/static/css/xadmin.css">    <link rel="stylesheet" href="https://unpkg.com/element-ui/lib/theme-chalk/index.css"></head><body><body class="login-bg">    <div class="login layui-anim layui-anim-up" id="app">        <div class="message" style="background: #EE7600">唯美图吧 - 注册小程序</div>        <div id="darkbannerwrap"></div>        <form action="" method="POST" class="layui-form" @submit.prevent="createApp">            <input name="name" v-model="form.name" placeholder="小程序名字"  type="text" lay-verify="required" class="layui-input" >            <hr class="hr15">            <input name="appKey" v-model="form.appKey" placeholder="app_id"  type="text" lay-verify="required" class="layui-input email" >            <hr class="hr15">            <input name="appSecret" v-model="form.appSecret" lay-verify="required" placeholder="app_secret"  type="text" class="layui-input">            <hr class="hr15">            <input name="mobile" v-model="form.mobile" lay-verify="required" placeholder="管理员手机号码"  type="text" class="layui-input">            <hr class="hr15">            <input value="提交" lay-submit lay-filter="login" style="width:100%;background: #EE7600" type="submit">            <hr class="hr20" >        </form>    </div></body><script src="https://cdn.bootcss.com/vue/2.5.16/vue.min.js"></script><script src="https://cdn.bootcss.com/axios/0.17.1/axios.min.js"></script><script type="text/javascript" src="https://cdn.bootcss.com/jquery/3.2.1/jquery.min.js"></script><script src="/static/lib/layui/layui.js" charset="utf-8"></script><script type="text/javascript" src="/static/js/xadmin.js"></script><script>    'use strict';    var app = new Vue({        el: '#app',        data: {            form:{}        },        created:function () {        },        methods: {            /**             * 提交数据             *             * @author 叶子             */            createApp:function () {                let formData = new FormData();                formData.append('name', this.form.name);                formData.append('appKey', this.form.appKey);                formData.append('appSecret', this.form.appSecret);                formData.append('mobile', this.form.region);                let config = {                    headers: {                        'Content-Type': 'multipart/form-data'                    }                }                axios.post("/admin/app/create",formData,config).then( res=> {                    console.log(res.data);                    var resData = res.data;                    if(resData.code === 0){                        window.location.href = "/admin/index";                    }else{                        layer.msg(resData.message)                    }                }).catch(function (error) {                    console.log(error);                });            }        },    })</script></html>